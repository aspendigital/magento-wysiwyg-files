<?xml version="1.0" encoding="UTF-8"?>
<config>
  <modules>
    <AspenDigital_WysiwygFiles>
      <version>1.0.0</version>
    </AspenDigital_WysiwygFiles>
  </modules>
  <adminhtml>
    <cms>
      <browser>
        <extensions>
          <allowed>
              <jpg>1</jpg>
              <jpeg>1</jpeg>
              <png>1</png>
              <gif>1</gif>
              <pdf>1</pdf>
          </allowed>
          <image_allowed>
              <jpg>1</jpg>
              <jpeg>1</jpeg>
              <png>1</png>
              <gif>1</gif>
          </image_allowed>
          <media_allowed>
              <flv>1</flv>
              <swf>1</swf>
              <avi>1</avi>
              <mov>1</mov>
              <rm>1</rm>
              <wmv>1</wmv>
          </media_allowed>
        </extensions>

      </browser>
    </cms>
  </adminhtml>
</config>